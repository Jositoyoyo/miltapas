<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="frontBundle\Entity\Img" table="img">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="file" type="string" column="file" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="path" type="string" column="path" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="local" type="integer" column="local" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="usuario" type="integer" column="usuario" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
